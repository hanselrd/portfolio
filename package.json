{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build:next": "next build",
    "build:spago": "spago build",
    "build": "run-s build:spago build:next",
    "start": "next start",
    "lint:next": "next lint",
    "lint:prettier:0": "prettier '**/*.{html,css,js,jsx,ts,tsx,json,md}' --check",
    "lint:prettier:1": "prettier --parser json '**/.*rc' --check",
    "lint:prettier": "run-s lint:prettier:*",
    "lint:purs-tidy": "purs-tidy check '**/*.purs'",
    "lint": "run-s lint:*",
    "format:prettier:0": "prettier '**/*.{html,css,js,jsx,ts,tsx,json,md}' --write",
    "format:prettier:1": "prettier --parser json '**/.*rc' --write",
    "format:prettier": "run-s format:prettier:*",
    "format:purs-tidy": "purs-tidy format-in-place '**/*.purs'",
    "format": "run-s format:*"
  },
  "dependencies": {
    "@next/font": "13.1.1",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.8",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "autoprefixer": "^10.4.13",
    "eslint": "8.30.0",
    "eslint-config-next": "13.1.1",
    "eslint-config-prettier": "^8.5.0",
    "next": "13.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "purescript": "^0.15.7",
    "purescript-psa": "^0.8.2",
    "purs-tidy": "^0.9.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "spago": "^0.20.9",
    "tailwindcss": "^3.2.4",
    "tailwindcss-dir": "^4.0.0",
    "typescript": "4.9.4"
  }
}
