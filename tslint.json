{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_module/**/*.ts",
      "coverage/lcov-report/*.js",
      "src/serviceWorker.ts"
    ]
  },
  "jsRules": {},
  "rules": {
    "no-console": false,
    "object-literal-sort-keys": false,
    "triple-equals": [true, "allow-null-check"],
    "no-empty": false,
    "no-bitwise": false
  },
  "rulesDirectory": []
}
